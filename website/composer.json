{
    "type": "project",
    "require": {
        "php": "^8.3",
        "blade-ui-kit/blade-heroicons": "^2.4",
        "blade-ui-kit/blade-icons": "^1.6",
        "felixdorn/php-color": "^1.0",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "mariosimao/notion-sdk-php": "^1.14",
        "spatie/laravel-export": "^1.1"
    },
    "require-dev": {
        "brianium/paratest": "^7.5",
        "fakerphp/faker": "^1.23",
        "itsgoingd/clockwork": "^5.2",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "pint",
        "test:lint": "pint --test",
        "test:types": "phpstan analyse --ansi --memory-limit=-1",
        "test:unit": "phpunit --order-by=defects --testsuite Feature",
        "test:e2e": "phpunit --order-by=defects --testsuite Browser",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
